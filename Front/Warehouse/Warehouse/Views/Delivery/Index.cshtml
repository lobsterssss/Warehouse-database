@{
}

<article>
    <section>
        <a asp-controller="Delivery" asp-action="Create">
            <button>Create Delivery</button>
        </a>
    </section>
    <Item-Container>
        @if (Model.Count() > 0)
        {
            @foreach (DeliveryViewModel delivery in Model)
            {
                @Html.Partial("Partials/DeliveryPartial", delivery)
            }
        }
        else
        {
            <h2>No Shelves found</h2>
        }
        </Warehouse-Container>
</article>