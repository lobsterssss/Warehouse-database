@model List<StoreViewModel>


<article>
    <Item-Container>
        @if (Model.Count() > 0)
        {
            @foreach (StoreViewModel store in Model)
            {
                @Html.Partial("Partials/StorePartial", store)
            }
        }
        else
        {
            <h2>No Stores found</h2>
        }
    </Item-Container>
</article>